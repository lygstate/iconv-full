{
  "name": "iconv-full",
  "description": "Convert character encodings in pure javascript, all encodings.",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "iconv",
    "convert",
    "charset",
    "icu"
  ],
  "scripts": {
    "test": "gulp test",
    "mocha-all": "mocha -r test test/test_*.js",
    "mocha": "mocha -r test",
    "mocha-debug": "node-debug --hidden node_modules/ node_modules/mocha/bin/_mocha -r test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lygstate/iconv-full.git"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "author": "Yonggang Luo <luoyonggang@gmail.com>",
  "contributors": [],
  "main": "./lib/index.js",
  "bugs": {
    "url": "https://github.com/mozilla-comm/jsmime/issues"
  },
  "browser": {
    "./extend-node": false,
    "./streams": false
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-plugin-transform-es2015-destructuring": "^6.4.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.0",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-babel-istanbul": "^0.11.0",
    "gulp-clean": "^0.3.1",
    "gulp-mocha": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "mocha": "^2.3.4",
    "requirejs": "^2.1.22",
    "requirejs-babel": "0.0.8",
    "spec-xunit-file": "0.0.1-3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "__URI__",
      "Components",
      "Globals",
      "Blob",
      "URL",
      "FormData",
      "describe",
      "dump",
      "expect",
      "it",
      "setup",
      "suite",
      "test",
      "define"
    ]
  },
  "dependencies": {
    "iconv-lite": "lygstate/iconv-lite.git"
  }
